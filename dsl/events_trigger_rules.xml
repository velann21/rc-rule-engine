<roulette>
    <ruleset name="eventRules" dataKey="MyData" resultKey="result" filterTypes="models.Events"
             filterStrict="false" prioritiesCount="all"  workflow="AddNodeCycle">
        <rule name="addNodeRules" priority="3">
            <r>with .MyData</r>
            <r>
                eq .models.Events.EventType "AddNode"|
                and (gt .models.Events.EventOccured 2) |
                .models.Events.SetAlertType "Mail" |
                .models.Events.SetTimeFrame "gt" 1
            </r>
            <r>end</r>
        </rule>
    </ruleset>



</roulette>